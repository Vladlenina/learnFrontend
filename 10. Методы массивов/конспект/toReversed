//Метод toReversed()эявляется копирующим аналогом метода reverse(). Он возвращает новый массив с элементами в обратном порядке.

const items = [1, 2, 3];
console.log(items); // [1, 2, 3]

const reversedItems = items.toReversed();
console.log(reversedItems); // [3, 2, 1]
console.log(items); // [1, 2, 3]
